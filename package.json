{
  "name": "NSU-Stevnebehandler",
  "version": "0.0.1",
  "author": "Thomas <tholav@gmail.com>",
  "description": "NSU resultat- og stevne- behandler",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "tests": "jest --verbose",
    "tests:watch": "jest --watch",
    "tests:coverage": "jest --verbose --coverage",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue --fix src",
    "postinstall": "npm run lint:fix",
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "build": {
    "appId": "io.tinyapp.nsu",
    "productName": "NSU-Stevnebehandler",
    "artifactName": "NSU-Stevnebehandler-${version}.${ext}",
    "copyright": "Copyright Â© NSU / Thomas Laaver",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": {
        "target": "zip"
      }
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": {
        "target": "nsis"
      }
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@sentry/electron": "^0.16.0",
    "electron-log": "^3.0.1",
    "lodash.merge": "^4.6.1",
    "pouchdb-adapter-idb": "^7.0.0",
    "pouchdb-adapter-memory": "^7.0.0",
    "rxdb": "^8.0.4",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "vue": "^2.5.22",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.9.0",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@sentry/cli": "^1.40.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-download": "^4.1.1",
    "element-ui": "^2.6.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^1.1.11",
    "flush-promises": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "ionicons": "^4.4.8",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^1.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "require-dir": "^1.0.0",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tailwindcss": "^0.7.2",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
