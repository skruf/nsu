{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        [ "env", { "targets": { "node": "current" } } ],
        "stage-0"
      ],
      "plugins": [ "istanbul" ]
    },

    "main": {
      "presets": [
        [ "env", { "targets": { "node": "current" } } ],
        "stage-0"
      ]
    },

    "renderer": {
      "presets": [
        [ "env", { "modules": false } ],
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        [ "component", {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        } ]
      ]
    },

    "web": {
      "presets": [
        [ "env", { "modules": false } ],
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        [ "component",
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        } ]
      ]
    }
  },
  "plugins": [ "transform-runtime" ]
}
